{
	"restartable": "rs",
	"ignore": [
		".git",
		".idea",
		".jsonenv",
		".micro-build",
		"logs",
		"node_modules",
		"jspm_packages",
		".npm",
		"public",
		"package.json",
		"package-lock.json"
	],
	"verbose": false,
	"execMap": {
		"js": "node"
	},
	"events": {
		"start": "echo -e \" ::: start :::\"",
		"// start": "echo -e \" ::: start :::\\e]0;${PROJECT_NAME} - MICRO-BUILD: waitting for compile\\007\"",
		"restart": "echo -ne '\\ec\\e[38;5;14m-------------------restart-------------------\\e[0m\\n' ; echo -ne \"\\e]0;${PROJECT_NAME} - MICRO-BUILD: debugging in $(pwd)\\007\""
	},
	"ext": "js json"
}
