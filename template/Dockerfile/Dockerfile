#{FROM}

RUN mkdir -p /data

#{NPM_INSTALL_INSTRUCTIONS}

#{CUSTOM_BUILD_BEFORE}

#{JSON_ENV_PASS}

#{BUILD_ARGUMENTS}

#{LABEL_INSTRUCTIONS}
#{NSG_LABEL_INSTRUCTIONS}

#{VOLUMES}

COPY . /data
WORKDIR /data

#{COMPILE_PLUGIN}

#{CUSTOM_BUILD_AFTER}
#{PORTS}

ARG COMMAND_LINE_ARGS
CMD "@{COMMAND}" $COMMAND_LINE_ARGS

ENTRYPOINT @{SHELL}
